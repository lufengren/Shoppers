<div class="container">
    {{#paginationData}}
    {{#disabled}}
    <span class="page-item" data-value="{{value}}" disabled>{{name}}</span>
    {{/disabled}}
    {{^disabled}}
    {{#active}}
    <span class="active page-item" data-value="{{value}}">{{name}}</span>
    {{/active}}
    {{^active}}
    <span class="page-item" data-value="{{value}}">{{name}}</span>
    {{/active}}
    {{/disabled}}
    {{/paginationData}}
    <span>
        {{pageNumber}} / {{pages}}
    </span>
</div>
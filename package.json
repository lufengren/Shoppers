{
  "name": "shoppers-frontend",
  "version": "1.0.0",
  "description": "shoppers front-end code",
  "private": true,
  "jest": {
    "moduleFileExtensions": ["js", "jsx"],
    "moduleDirectories": ["node_modules", "bower_components", "shared"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "^service(.*)$": "<rootDir>/src/service/userService.js"
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "scripts": {
    "test": "jest",
    "build": "webpack",
    "start": "webpack-dev-server --inline --content-base .",
    "dev": "webpack --mode development",
    "prod": "webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lufengren/Shoppers-Frontend.git"
  },
  "author": "Lucia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lufengren/Shoppers-Frontend/issues"
  },
  "homepage": "https://github.com/lufengren/Shoppers-Frontend#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-jest": "^24.9.0",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.6.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.10.1",
    "hogan.js": "^3.0.2",
    "json-server": "^0.15.0"
  }
}